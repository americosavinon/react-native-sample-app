    <style name="TextAppearance.AmericanWell.Micro.White" parent="TextAppearance.AmericanWell.Micro">
        <item name="android:textColor">@color/awsdk_color_white</item>
    </style>

    <style name="TextAppearance.AmericanWell.Small.Inverse" parent="@android:style/TextAppearance.Inverse">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/awsdk_text_size_body</item>
    </style>

    <style name="TextAppearance.AmericanWell.Small.DarkGray"><!--parent="@style/TextAppearance.AmericanWell.Small"-->
        <item name="android:textColor">@color/awsdk_color_darkgray</item>
    </style>

    <style name="TextAppearance.AmericanWell.Medium.Inverse" parent="@android:style/TextAppearance.Inverse">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/awsdk_text_size_subhead</item>
    </style>

    <style name="TextAppearance.AmericanWell.MediumLarge.Inverse" parent="@android:style/TextAppearance.Inverse">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/textSizeMediumLarge</item>
    </style>

    <style name="TextAppearance.AmericanWell.Large.Inverse" parent="@android:style/TextAppearance.Inverse">
        <item name="android:textColor">@android:color/white</item>

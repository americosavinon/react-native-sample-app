<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:textDirection">locale</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.NoTitleBar">
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

    <style name="TextAppearance.AmericanWell" parent="@style/TextAppearance.AppCompat"/>

    <style name="TextAppearance.AmericanWell.Small">
        <item name="android:textColor">@color/awsdk_color_black</item>
        <item name="android:textSize">@dimen/awsdk_text_size_body</item>
    </style>

    <style name="TextAppearance.AmericanWell.Small.Charcoal" parent="TextAppearance.AmericanWell.Small">
        <item name="android:textColor">@color/charcoal</item>
    </style>

    <style name="TextAppearance.AmericanWell.Small.MediumGrey" parent="TextAppearance.AmericanWell.Small">
        <item name="android:textColor">@color/medium_grey</item>
    </style>

    <style name="TextAppearance.AmericanWell.Medium">
        <item name="android:textColor">@color/awsdk_color_black</item>
        <item name="android:textSize">@dimen/awsdk_text_size_subhead</item>
    </style>

    <style name="TextAppearance.AmericanWell.MediumLarger">

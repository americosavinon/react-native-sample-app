    </style>

    <style name="TextAppearance.AmericanWell.MediumLarge.Primary" parent="TextAppearance.AmericanWell.MediumLarge">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="TextAppearance.AmericanWell.MediumLarge.Primary.Selected" parent="TextAppearance.AmericanWell.MediumLarge.Primary">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextAppearance.AmericanWell.MediumLarge.Gray" parent="TextAppearance.AmericanWell.MediumLarge">
        <item name="android:textColor">@color/color_text_gray</item>
    </style>

    <style name="PasswordHintOkStyle" parent="TextAppearance.AmericanWell">
        <item name="android:textColor">@color/awsdk_color_darkgray</item>
        <item name="android:textSize">@dimen/textSizeMicro</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:gravity">start</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:drawableStart">@drawable/ic_checkmark_green</item>
        <item name="android:drawablePadding">4dp</item>
    </style>

    <style name="PasswordHintStyle.Error" parent="PasswordHintOkStyle">
        <item name="android:textColor">@color/cherry</item>
        <item name="android:drawableStart">@drawable/ic_fail_red</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/colorAccent</item>
        <item name="colorControlActivated">@color/color_link</item>
    </style>

    <style name="TextAppearance.AmericanWell.ExtraSmall">
        <item name="android:textColor">@color/awsdk_color_black</item>
        <item name="android:textSize">@dimen/textSizeExtraSmall</item>
    </style>

    <style name="InputLabel" parent="TextAppearance.Design.Hint">
        <item name="android:paddingBottom">@dimen/input_label_vertical_spacing</item>
        <item name="android:paddingStart">@dimen/input_label_horizontal_spacing</item>
        <item name="android:paddingEnd">@dimen/input_label_horizontal_spacing</item>
    </style>

    <style name="Widget.AmericanWell.AppCompat.Button.Colored" parent="Widget.AppCompat.Button.Colored">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAlignment">center</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:minWidth">48dp</item>
    </style>

    <style name="DatePickerDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="TimePickerDialog" parent="DatePickerDialog" />

    <!--New Styles - Going forward all UI should use these styles-->

